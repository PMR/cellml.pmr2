<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="pmr2">

  <!-- Browser pages -->

  <browser:page
      for="pmr2.app.content.ExposureFile"
      name="basic_ccode"
      class=".view.BasicCCodeNoteView"
      permission="zope2.View"
      />

  <browser:page
      for="pmr2.app.content.ExposureFile"
      name="cmeta"
      class=".view.CmetaNoteView"
      permission="zope2.View"
      />

  <browser:page
      for="pmr2.app.content.ExposureFile"
      name="cellml_codegen"
      class=".view.CellMLCodegenNoteView"
      permission="zope2.View"
      />

  <browser:page
      for="pmr2.app.content.ExposureFile"
      name="opencellsession"
      class=".view.OpenCellSessionNoteView"
      permission="zope2.View"
      />

  <!--
    CellML requires application specific view.
  -->
  <browser:page
      for="pmr2.app.content.Workspace"
      name="pcenv"
      class=".view.WorkspaceRawfileXmlBasePCEnvView"
      permission="zope2.View"
      />

</configure>
